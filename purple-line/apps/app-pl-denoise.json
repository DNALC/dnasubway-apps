            "semantics": {
                "ontology": [
                    "xs:string"
                ],
                "fileTypes": [
                    "raw-0"
                ]
            }
        }
    ],
    "parameters": [
        {
            "id": "jobName",
            "value": {
                "default": "qiime2-step2",
                "type": "string",
                "validator": "",
                "visible": true,
                "required": true
            },
            "details": {
                "label": "job name",
                "description": "the name of the job"
            },
            "semantics": {
                "ontology": [
                    "xs:string"
                ]
            }
        },
        {
            "id": "paired",
            "value": {
                "default": false,
                "type": "bool",
                "visible": true,
                "required": false
            },
            "details": {
                "label": "Provided files are from a paired-end experiment (L and R)",
                "description": "Provided files are from a paired-end experiment (L and R)"
            },
            "semantics": {
                "ontology": [
                    "xs:boolean"
                ]
            }
        },
        {
            "id": "truncLen",
            "value": {
                "default": 0,
                "type": "number",
                "validator": "[0-9]+",
                "visible": true,
                "required": true
            },
            "details": {
                "label": "Position at which sequences should be truncated due to decrease inquality.",
                "description": "Position at which sequences should be truncated due to decrease in quality. This truncates the 3' end of the of the input sequences, which will be the bases that were sequenced in the last cycles. Reads that are shorter than this value will be discarded"
            },
            "semantics": {
                "ontology": [
                    "xs:integer"
                ]
            }
        },
        {
            "id": "trimLeft",
            "value": {
                "default": 0,
                "type": "number",
                "validator": "[0-9]+",
                "visible": true,
                "required": false
            },
            "details": {
                "label": "Position at which sequences",
                "description": "Position at which sequences should be trimmed due to low quality. This trims the 5' end of the of the input sequences, which will be the bases that were sequenced in the first cycles."
            },
            "semantics": {
                "ontology": [
                    "xs:integer"
                ]
            }
        }
    ],
    "outputs": []
